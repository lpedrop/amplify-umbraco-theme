<?xml version="1.0" encoding="utf-8"?>
<Amplify>
  <BackOffice>
    <!-- These are read and used to configure Amplify Theme -->
    <Sass />
    <Gradients>
      <Var Alias="$wiretap" StartColor="hsl(302, 60%, 34%)" EndColor="hsl(23, 89%, 54%)" Degree="90" Order="3" />
      <Var Alias="$grayfade" StartColor="hsl(0, 0%, 100%)" EndColor="hsl(0, 0%, 94%)" Degree="90" Order="2" />
      <Var Alias="$amplified" StartColor="hsl(198, 100%, 45%)" EndColor="hsl(327, 100%, 45%)" Degree="90" Order="1" />
      <Var Alias="$reverse-amplified" StartColor="hsl(284, 76%, 62%)" EndColor="hsl(198, 100%, 45%)" Degree="90" Order="0" />
    </Gradients>
    <Swatch>
      <Var Alias="$primary" DefaultVal="$primary" Enabled="true" Order="8"></Var>
      <Var Alias="$link" DefaultVal="$link" Enabled="true" Order="7"></Var>
      <Var Alias="$info" DefaultVal="info" Enabled="true" Order="6"></Var>
      <Var Alias="$dark" DefaultVal="$dark" Enabled="true" Order="5"></Var>
      <Var Alias="$white" DefaultVal="white" Enabled="true" Order="4"></Var>
      <Var Alias="$white-ter" DefaultVal="$white-ter" Enabled="true" Order="3"></Var>
      <Var Alias="$grey-lighter" DefaultVal="$grey-lighter" Enabled="true" Order="2"></Var>
      <Var Alias="$grey-light" DefaultVal="$grey-light" Enabled="true" Order="1"></Var>
      <Var Alias="$grey" DefaultVal="$grey" Enabled="true" Order="0"></Var>
      <Var Alias="$blue-light" DefaultVal="$blue-light" Enabled="true" Order="0"></Var>
      <Var Alias="$blue-darkest" DefaultVal="$blue-darkest" Enabled="true" Order="0"></Var>
      <Var Alias="$blue-grey-dark" DefaultVal="$blue-grey-dark" Enabled="true" Order="0"></Var>
      <Var Alias="$blue-dark" DefaultVal="$blue-dark" Enabled="true" Order="0"></Var>
    </Swatch>
    <Vars>
      <Var Alias="$primary" Type="themecolor" DefaultVal="$turquoise" Enabled="true" Order="0">$pastel</Var>
      <Var Alias="$link" Type="themecolor" DefaultVal="$blue" Enabled="true" Order="1">$purple</Var>
      <Var Alias="$info" Type="themecolor" DefaultVal="$cyan" Enabled="true" Order="2">$blue</Var>
      <Var Alias="$success" Type="themecolor" DefaultVal="$green" Enabled="true" Order="3">$green</Var>
      <Var Alias="$warning" Type="themecolor" DefaultVal="$yellow" Enabled="true" Order="4">$yellow</Var>
      <Var Alias="$danger" Type="themecolor" DefaultVal="$red" Enabled="true" Order="5">$red</Var>
      <Var Alias="$dark" Type="themecolor" DefaultVal="$grey-darker" Enabled="true" Order="6">$grey-darker</Var>
      <Var Alias="$text" Type="themecolor" DefaultVal="$grey-dark" Enabled="true" Order="7">$grey-dark</Var>
      <Var Alias="$black" Type="color" DefaultVal="hsl(0, 0%, 4%)" Enabled="true" Order="24">hsl(0, 0%, 4%)</Var>
      <Var Alias="$black-bis" Type="color" DefaultVal="hsl(0, 0%, 7%)" Enabled="true" Order="23">hsl(0, 0%, 7%)</Var>
      <Var Alias="$black-ter" Type="color" DefaultVal="hsl(0, 0%, 14%)" Enabled="true" Order="22">hsl(0, 0%, 14%)</Var>
      <Var Alias="$grey-darker" Type="color" DefaultVal="hsl(0, 0%, 21%)" Enabled="true" Order="21">hsl(0, 0%, 21%)</Var>
      <Var Alias="$grey-dark" Type="color" DefaultVal="hsl(0, 0%, 29%)" Enabled="true" Order="20">hsl(0, 0%, 29%)</Var>
      <Var Alias="$grey" Type="color" DefaultVal="hsl(0, 0%, 48%)" Enabled="true" Order="19">hsl(0, 0%, 48%)</Var>
      <Var Alias="$grey-light" Type="color" DefaultVal="hsl(0, 0%, 71%)" Enabled="true" Order="18">hsl(0, 0%, 71%)</Var>
      <Var Alias="$grey-lighter" Type="color" DefaultVal="hsl(0, 0%, 86%)" Enabled="true" Order="17">hsl(0, 0%, 86%)</Var>
      <Var Alias="$grey-lightest" Type="color" DefaultVal="hsl(0, 0%, 93%)" Enabled="true" Order="16">hsl(0, 0%, 93%)</Var>
      <Var Alias="$white-ter" Type="color" DefaultVal="hsl(0, 0%, 96%)" Enabled="true" Order="15">hsl(0, 0%, 96%)</Var>
      <Var Alias="$white-bis" Type="color" DefaultVal="hsl(0, 0%, 98%)" Enabled="true" Order="14">hsl(0, 0%, 98%)</Var>
      <Var Alias="$white" Type="color" DefaultVal="hsl(0, 0%, 100%)" Enabled="true" Order="13">hsl(0, 0%, 100%)</Var>
      <Var Alias="$orange" Type="color" DefaultVal="hsl(14, 100%, 53%)" Enabled="true" Order="12">hsl(14, 100%, 53%)</Var>
      <Var Alias="$yellow" Type="color" DefaultVal="hsl(48, 100%, 67%)" Enabled="true" Order="11">hsl(48, 100%, 67%)</Var>
      <Var Alias="$green" Type="color" DefaultVal="hsl(141, 53%, 53%)" Enabled="true" Order="10">hsl(141, 53%, 53%)</Var>
      <Var Alias="$turquoise" Type="color" DefaultVal="hsl(171, 100%, 41%)" Enabled="true" Order="9">hsl(171, 100%, 41%)</Var>
      <Var Alias="$cyan" Type="color" DefaultVal="hsl(204, 71%, 53%)" Enabled="true" Order="8">hsl(204, 71%, 53%)</Var>
      <Var Alias="$blue" Type="color" DefaultVal="hsl(217, 71%, 53%)" Enabled="true" Order="5">hsl(193, 78%, 53%)</Var>
      <Var Alias="$purple" Type="color" DefaultVal="hsl(271, 100%, 71%)" Enabled="true" Order="6">hsl(327, 100%, 45%)</Var>
      <Var Alias="$red" Type="color" DefaultVal="hsl(348, 86%, 61%)" Enabled="true" Order="7">hsl(348, 86%, 61%)</Var>
      <Var Alias="$pastel" Type="color" DefaultVal="hsl(300, 24%, 80%)" Enabled="true" Order="4">hsl(185, 100%, 96%)</Var>
      <Var Alias="$blue-light" Type="color" DefaultVal="hsl(198, 81%, 78%)" Enabled="true" Order="0">hsl(198, 81%, 78%)</Var>
      <Var Alias="$blue-darkest" Type="color" DefaultVal="hsl(223, 45%, 8%)" Enabled="true" Order="1">hsl(223, 45%, 8%)</Var>
      <Var Alias="$blue-grey-dark" Type="color" DefaultVal="hsl(225, 27%, 23%)" Enabled="true" Order="2">hsl(225, 27%, 23%)</Var>
      <Var Alias="$blue-dark" Type="color" DefaultVal="hsl(224, 33%, 16%)" Enabled="true" Order="3">hsl(224, 33%, 16%)</Var>
    </Vars>
    <FontStacks>
      <FontStack Alias="$family-primary" Type="themefont" DefaultVal="$family-sans-serif" Enabled="true" Order="0">$family-baloo</FontStack>
      <FontStack Alias="$family-secondary" Type="themefont" DefaultVal="$family-sans-serif" Enabled="true" Order="1">$family-sans-serif</FontStack>
      <FontStack Alias="$family-sans-serif" Type="font" DefaultVal="Roboto" Enabled="true" order="0" Order="1">BlinkMacSystemFont, -apple-system, "Segoe UI", "Roboto", "Oxygen", "Ubuntu", "Cantarell", "Fira Sans", "Droid Sans", "Helvetica Neue", "Helvetica", "Arial", sans-serif</FontStack>
      <FontStack Alias="$family-monospace" Type="font" DefaultVal="Roboto Mono" Enabled="true" Order="2">monospace</FontStack>
      <FontStack Alias="$family-baloo" Type="font" DefaultVal="'Baloo Thambi 2', BlinkMacSystemFont, -apple-system, &quot;Segoe UI&quot;, &quot;Roboto&quot;, &quot;Oxygen&quot;, &quot;Ubuntu&quot;, &quot;Cantarell&quot;, &quot;Fira Sans&quot;, &quot;Droid Sans&quot;, &quot;Helvetica Neue&quot;, &quot;Helvetica&quot;, &quot;Arial&quot;, sans-serif" Enabled="true" Order="0">'Baloo Thambi 2', BlinkMacSystemFont, -apple-system, "Segoe UI", "Roboto", "Oxygen", "Ubuntu", "Cantarell", "Fira Sans", "Droid Sans", "Helvetica Neue", "Helvetica", "Arial", sans-serif</FontStack>
    </FontStacks>
    <Fonts>
      <Font Alias="$font-baloo-thambi-2" Type="display" Enabled="true" Order="0">Baloo Thambi 2</Font>
    </Fonts>
  </BackOffice>
</Amplify>