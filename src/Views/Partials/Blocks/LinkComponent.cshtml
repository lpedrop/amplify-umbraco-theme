@inherits Umbraco.Web.Mvc.UmbracoViewPage<ButtonsComposition>
@using Amplify.Web.Custom.Extensions

@if (Model.HasValue())
{
    <div class="column is-12">
        <div class="columns is-centered amplify-button">
            <div class="column is-one-third has-text-centered has-margin-top-6-desktop has-margin-top-4 has-margin-bottom-4" data-aos="zoom-in">
                <div class="buttons is-centered">
                    @RazorHelper.RenderLinkButtons(Model.Buttons,"is-medium")
                </div>
            </div>
        </div>
    </div>
}